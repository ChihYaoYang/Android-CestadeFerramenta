<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.senac.cestadeferramenta">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera.any"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.Camera.autofocus"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Produto"
            android:label="Produto"></activity>
        <activity
            android:name=".Listagem"
            android:label="@string/title_activity_listagem"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Sobre"
            android:label="Sobre" />
        <activity
            android:name=".Conversao"
            android:label="Conversão Byte" />
        <activity
            android:name=".Calculo"
            android:label="Cálculo IMC"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>